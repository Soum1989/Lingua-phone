{
  "name": "multilingual-chatbot",
  "version": "1.0.0",
  "description": "Multilingual AI chatbot with gamified pronunciation training for GKEturns10 Hackathon",
  "type": "module",
  "scripts": {
    "dev": "npm run build && npm run dev:server",
    "dev:client": "vite --port 5174",
    "dev:server": "tsx watch server/index.ts",
    "build": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "vite preview",
    "lint": "eslint .",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "lucide-react": "^0.344.0",
    "framer-motion": "^10.16.16",
    "@hookform/resolvers": "^3.3.2",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "tsx": "^4.7.0",
    "concurrently": "^8.2.2",
    "vite": "^5.4.2",
    "@eslint/js": "^9.9.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.3.0"
  },
  "keywords": ["multilingual", "chatbot", "AI", "pronunciation", "gamification", "hackathon"],
  "author": "GKEturns10 Hackathon Participant",
  "license": "MIT"
}